{
  "name": "@pague-dev/sdk-node",
  "version": "0.1.4",
  "description": "Node.js SDK for the pague.dev API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsdown src/index.ts --format esm,cjs --dts",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "pague.dev",
    "pix",
    "payments",
    "brazil",
    "sdk"
  ],
  "author": "pague.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pague-dev/sdk-node"
  },
  "homepage": "https://docs.pague.dev",
  "bugs": {
    "url": "https://github.com/pague-dev/sdk-node/issues"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@types/node": "^22.0.0",
    "tsdown": "^0.18.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0",
    "vitest-fetch-mock": "^0.4.0"
  }
}
